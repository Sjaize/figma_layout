{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2020",
    "lib": [
      "es2020"
    ],
    "outDir": "out",
    "rootDir": "src",
    "strict": true,
    "sourceMap": true,

    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "types": [
      "node",
      "ws"
    ]
  },
  "exclude": [
    "node_modules",
    ".vscode-test",
	"src/test"
  ]
}
